services:
  basketapi:
    build:
      context: C:\Keroles Shehata\2- Programming\GitHub\Live\Kero.MicroservicesApp
      dockerfile: src/Basket/Basket.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Basket.API
      target: base
    container_name: basketapi
    depends_on:
      basketdb:
        condition: service_started
      rabbitmq:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      CacheSettings:ConnectionString: basketdb:6379
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      EventBusSettings:HostAddress: amqp://guest:guest@rabbitmq:5672
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: basketapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/Basket.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8001
      target: 80
    tty: true
    volumes:
    - C:\Keroles Shehata\2- Programming\GitHub\Live\Kero.MicroservicesApp\src\Basket\Basket.API:/app:rw
    - C:\Users\kshehata\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\kshehata\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\kshehata\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\kshehata\.nuget\packages:/root/.nuget/packages:ro
    - C:\Keroles Shehata\2- Programming\GitHub\Live\Kero.MicroservicesApp:/src:rw
  basketdb:
    container_name: basketdb
    image: redis:alpine
    ports:
    - published: 6379
      target: 6379
    restart: always
  catalogapi:
    build:
      context: C:\Keroles Shehata\2- Programming\GitHub\Live\Kero.MicroservicesApp
      dockerfile: src/Catalog/Catalog.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Catalog.API
      target: base
    container_name: catalogapi
    depends_on:
      catalogdb:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      DatabaseSettings:ConnectionString: mongodb://catalogdb:27017
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: catalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/Catalog.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8000
      target: 80
    tty: true
    volumes:
    - C:\Keroles Shehata\2- Programming\GitHub\Live\Kero.MicroservicesApp\src\Catalog\Catalog.API:/app:rw
    - C:\Users\kshehata\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\kshehata\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\kshehata\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\kshehata\.nuget\packages:/root/.nuget/packages:ro
    - C:\Keroles Shehata\2- Programming\GitHub\Live\Kero.MicroservicesApp:/src:rw
  catalogdb:
    container_name: catalogdb
    image: mongo
    ports:
    - published: 27017
